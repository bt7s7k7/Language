template(T is any 0)
function printf(templ: T): Void => extern

namespace Foo {
    struct {
        i: Number
    }
}

function main() {
    var foo = Foo {i: 0}
    printf(`i++ init ${foo.i}; ret ${foo.i++}; now ${foo.i}`)
    printf(`i-- init ${foo.i}; ret ${foo.i--}; now ${foo.i}`)
    printf(`++i init ${foo.i}; ret ${++foo.i}; now ${foo.i}`)
    printf(`--i init ${foo.i}; ret ${--foo.i}; now ${foo.i}`)

    /* for (var i = 0; i < 10; i++) {
        printf(`${i}`)
    } */
}